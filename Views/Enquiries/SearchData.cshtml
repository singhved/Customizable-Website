@{
    string FullName = ViewBag.FullName;
    if (FullName == "")
    {
        FullName = null;
    }
    System.Data.DataTable dt = SecondWeb.Models.Procs.GetEnquiry(FullName);
}
@if (dt.Rows.Count > 0)
{
    for (int i = 0; i < dt.Rows.Count; i++)
    {
        <tr>
            <td st>@(i+1).</td>
            <td>@dt.Rows[i]["FullName"].ToString()</td>
            <td class="text-primary" onclick="ModalBtn('@dt.Rows[i]["FullName"].ToString()')" style="cursor:pointer">@dt.Rows[i]["Email"].ToString()</td>
            <td>@dt.Rows[i]["Subjects"].ToString()</td>
            <td>@dt.Rows[i]["Messages"].ToString()</td>
        </tr>
    }
}
else
{
    <h5 class="text-center text-primary" style="margin-right: -927px; margin-top: 100px">No data found...!</h5>
}
