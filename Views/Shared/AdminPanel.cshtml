    <script src="../../AdminAssets/assets/libs/node-waves/waves.min.js"></script>
    <script src="../../AdminAssets/assets/libs/feather-icons/feather.min.js"></script>
    <!-- pace js -->
    <script src="../../AdminAssets/assets/libs/pace-js/pace.min.js"></script>

    <!-- apexcharts -->
    <script src="../../AdminAssets/assets/libs/apexcharts/apexcharts.min.js"></script>

    <!-- Plugins js-->
    <script src="../../AdminAssets/assets/libs/admin-resources/jquery.vectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="../../AdminAssets/assets/libs/admin-resources/jquery.vectormap/maps/jquery-jvectormap-world-mill-en.js"></script>
